opover\"\n  },\n  data: function data() {\n    return {\n      //   exampleTwo\n      show: false,\n      // component-basic-usage\n      placements: [\"topright\", \"top\", \"topleft\", \"bottomright\", \"bottom\", \"bottomleft\", \"righttop\", \"right\", \"lefttop\", \"rightbottom\", \"left\", \"leftbottom\"]\n    };\n  }\n};",{"version":3,"sources":["popover.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4HA,eAAA;AACA,EAAA,QAAA,EAAA;AACA;AACA,IAAA,KAAA,EAAA;AAFA,GADA;AAKA,EAAA,IALA,kBAKA;AACA,WAAA;AACA;AACA,MAAA,IAAA,EAAA,KAFA;AAIA;AACA,MAAA,UAAA,EAAA,CACA,UADA,EAEA,KAFA,EAGA,SAHA,EAIA,aAJA,EAKA,QALA,EAMA,YANA,EAOA,UAPA,EAQA,OARA,EASA,SATA,EAUA,aAVA,EAWA,MAXA,EAYA,YAZA;AALA,KAAA;AAoBA;AA1BA,CAAA","sourcesContent":["<template>\n  <div class=\"main-content\">\n    <breadcumb :page=\"'Popover'\" :folder=\"'UI Kits'\" />\n\n    <b-row>\n      <b-col md=\"6\" class=\" mb-30\">\n        <b-card title=\"Example\">\n          <div class=\" my-3\">\n            <b-button\n              variant=\"danger\"\n              v-b-popover.hover=\"'I am popover content!'\"\n              title=\"Popover Title\"\n              >Hover Me</b-button\n            >\n          </div>\n        </b-card>\n      </b-col>\n      <b-col md=\"6\" class=\" mb-30\">\n        <b-card title=\"Example Two\">\n          <b-button\n            id=\"popover-button-sync\"\n            class=\"mb-4 \"\n            variant=\"success\"\n            >I have a popover</b-button\n          >\n\n          <b-button class=\"p-2 mb-4\" @click=\"show = !show\">Toggle Popover</b-button>\n\n          <b-popover\n            :show.sync=\"show\"\n            target=\"popover-button-sync\"\n            title=\"Popover\"\n          >\n            Hello <strong>World!</strong>\n          </b-popover>\n        </b-card>\n      </b-col>\n\n      <b-col md=\"12\" class=\" mb-30\">\n        <b-card title=\"Four Directions (HOVER)\">\n          <b-row class=\"text-left\">\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4\">\n              <b-button v-b-popover.hover.top=\"'Popover!'\" variant=\"primary\"\n                >Top</b-button\n              >\n            </b-col>\n\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4\">\n              <b-button v-b-popover.hover.right=\"'Popover!'\" variant=\"success\"\n                >Right</b-button\n              >\n            </b-col>\n\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4\">\n              <b-button v-b-popover.hover.left=\"'Popover!'\" variant=\"danger\"\n                >Left</b-button\n              >\n            </b-col>\n\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4\">\n              <b-button v-b-popover.hover.bottom=\"'ToolTip!'\" variant=\"warning\"\n                >Bottom</b-button\n              >\n            </b-col>\n          </b-row>\n        </b-card>\n      </b-col>\n\n      <b-col md=\"12\" class=\" mb-30\">\n        <b-card title=\"Placement\">\n          <b-row>\n            <b-col\n              v-for=\"placement in placements\"\n              :key=\"placement\"\n              md=\"2\"\n              sm=\"6\"\n              class=\"mb-4 text-left\"\n            >\n              <b-button :id=\"`popover-1-${placement}`\" variant=\"primary\">{{\n                placement\n              }}</b-button>\n              <b-popover\n                :target=\"`popover-1-${placement}`\"\n                :placement=\"placement\"\n                title=\"Popover!\"\n                triggers=\"hover focus\"\n                :content=\"`Placement ${placement}`\"\n              ></b-popover>\n            </b-col>\n          </b-row>\n\n          <h5 class=\"my-3\">Content via properties or slots</h5>\n          <b-row>\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4 text-left\">\n              <b-button id=\"popover-2\" variant=\"primary\"\n                >Using properties</b-button\n              >\n              <b-popover\n                target=\"popover-2\"\n                title=\"Prop Examples\"\n                triggers=\"hover focus\"\n                content=\"Embedding content using properties is easy\"\n              ></b-popover>\n            </b-col>\n\n            <b-col md=\"2\" sm=\"6\" class=\"mb-4 text-left\">\n              <b-button id=\"popover-3\" variant=\"primary\">Using slots</b-button>\n              <b-popover target=\"popover-3\" triggers=\"hover focus\">\n                <template slot=\"title\"\n                  >Content via Slots</template\n                >\n                Embedding content\n                <span class=\"text-danger\">using slots</span> affords you\n                <em>greater <strong>control.</strong></em> and basic HTML\n                support.\n              </b-popover>\n            </b-col>\n          </b-row>\n        </b-card>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n<script>\nexport default {\n  metaInfo: {\n    // if no subcomponents specify a metaInfo.title, this title will be used\n    title: \"Popover\"\n  },\n  data() {\n    return {\n      //   exampleTwo\n      show: false,\n\n      // component-basic-usage\n      placements: [\n        \"topright\",\n        \"top\",\n        \"topleft\",\n        \"bottomright\",\n        \"bottom\",\n        \"bottomleft\",\n        \"righttop\",\n        \"right\",\n        \"lefttop\",\n        \"rightbottom\",\n        \"left\",\n        \"leftbottom\"\n      ]\n    };\n  }\n};\n</script>"],"sourceRoot":"src/views/app/ui-kits"}]}