n                    <b-form-input id=\"input-1\" v-model=\"SName\" type=\"text\" required placeholder=\"Supplier Name\" ></b-form-input>\n                  </b-col>\n                </b-row>\n                <b-row class=\"ml-3 mr-3\">\n                  <b-col md=\"4\" class=\"mt-3\">\n                    <b-form-input id=\"input-1\" v-model=\"Street1\" type=\"text\" placeholder=\"Street 1\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"4\" class=\"mt-3\">\n                    <b-form-input id=\"input-1\" v-model=\"Street2\" type=\"text\" placeholder=\"Street 2\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"4\" class=\"mt-3\">\n                    <b-form-input id=\"input-1\" v-model=\"Street3\" type=\"text\" placeholder=\"Street 3\" ></b-form-input>\n                  </b-col>\n                </b-row>\n                <b-row class=\"ml-3 mr-3\">\n                  <b-col md=\"2\" class=\"mt-3\">\n                    <b-form-input id=\"input-1\" v-model=\"Zip\" type=\"text\" placeholder=\"ZIP\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"2\" class=\"mt-3\">\n                    <b-form-input id=\"input-2\" v-model=\"City\" type=\"text\" placeholder=\"City\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"4\" class=\"mt-3\">\n                    <b-form-input id=\"input-3\" v-model=\"Province\" type=\"text\" placeholder=\"Province\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"4\" class=\"mt-3\">\n                    <b-form-input id=\"input-4\" v-model=\"Land\" type=\"text\" required placeholder=\"Land\" ></b-form-input>\n                  </b-col>\n                </b-row>\n                <b-row class=\"ml-3 mr-3\">\n                  <b-col md=\"3\" class=\"mt-3\">\n                    <b-form-input id=\"input-5\" v-model=\"CPerson\" type=\"text\" placeholder=\"Contact Person\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"3\" class=\"mt-3\">\n                    <b-form-input id=\"input-6\" v-model=\"Phone\" type=\"text\" placeholder=\"Phone\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"3\" class=\"mt-3\">\n                    <b-form-input id=\"input-7\" v-model=\"Mail\" type=\"text\" placeholder=\"E-Mail\" ></b-form-input>\n                  </b-col>\n                  <b-col md=\"3\" class=\"mt-3\">\n                    <b-form-input id=\"input-8\" v-model=\"Skype\" type=\"text\" placeholder=\"Skype\" ></b-form-input>\n                  </b-col>\n                </b-row>\n                <b-row class=\"ml-3 mr-3\">\n                  <b-col md=\"6\" class=\"mt-3\">\n                    <b-button @click=\"emptyInput\" block variant=\"outline-primary\">Delete Input</b-button>\n                  </b-col>\n                  <b-col md=\"6\" class=\"mt-3\">\n                    <b-button @click=\"addSupplier\" block variant=\"primary\">Add Supplier</b-button>\n                  </b-col>\n                </b-row>\n                <b-row class=\"ml-3 mr-3\">\n                  <b-col md=\"12\" class=\"mt-3\">\n                    <b-alert v-if=\"error\" show variant=\"alert alert-card alert-danger\" dismissible>\n                      <strong class=\"text-capitalize\">Error!</strong> The Input is not valid.\n                    </b-alert>\n                  </b-col>\n                </b-row>\n              </b-form>\n            </b-collapse>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-md-12\">\n        <div class=\"card mb-30\">\n          <div class=\"card-body p-0 \">\n            <h5 class=\"card-title border-bottom p-3 mb-2\">Supplier</h5>\n            <vue-good-table ref=\"supplier-table\"\n              :columns=\"columns\"\n              :line-numbers=\"false\"\n              styleClass=\"order-table vgt-table striped\"\n              :rows=\"rows\"\n              :search-options=\"{\n                enabled: true\n              }\"\n              :select-options=\"{ \n                enabled: true,\n              }\"\n              :pagination-options=\"{\n                enabled: true,\n                perPage: 10,\n              }\">\n              <div slot=\"selected-row-actions\">\n                <b-button variant=\"outline-danger m-1\" @click=\"openModal\">Delete</b-button>\n              </div>\n            </vue-good-table>\n            <b-modal id=\"modal-1\" title=\"BootstrapVue\" v-model=\"show\">\n            </b-modal>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n  <!-- ============ Body content End ============= -->\n</template>\n<script>\nimport { apiService } from \"@/common/api.service.js\";\n\nexport default {\n  metaInfo: {    \n    title: \"Supplier\"\n  },\n  data() {\n    return {\n      suppliers: [],\n      show: false,\n      SName: '',\n      Street1: '',\n      Street2: '',\n      Street3: '',\n      Zip: '',\n      City: '',\n      Province: '',\n      Land: '',\n      CPerson: '',\n      Phone: '',\n      Mail: '',\n      Skype: '',\n      next: null,\n      error: false,\n      columns: [\n        // {\n        //   label: \"ID\",\n        //   field: \"id\",\n        //   width: '10px',\n        // },\n        {\n          label: \"Supplier\",\n          field: \"txtSupplierName\",\n          width: '50px',\n        },\n        {\n          label: \"Street 1\",\n          field: \"txtStreet1\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Street 2\",\n          field: \"txtStreet2\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Street 3\",\n          field: \"txtStreet3\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"ZIP\",\n          field: \"txtZip\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"City\",\n          field: \"txtCity\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Province\",\n          field: \"txtProvince\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Land\",\n          field: \"txtLand\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Contact Person\",\n          field: \"txtContactPerson\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Phone\",\n          field: \"txtCpPhone\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"E-Mail\",\n          field: \"txtCpMail\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n        {\n          label: \"Skype\",\n          field: \"txtCpSkype\",\n          thClass: \"text-left\",\n          tdClass: \"text-left\"\n        },\n      ],\n      rows: []\n    };\n  },\n  methods: {\n    getSuppliers() {\n      if (!this.error) {\n        let endpoint = `/api/supplier/`;\n        apiService(endpoint)\n          .then(data => {\n            this.suppliers.push(...data.results);\n            this.rows = this.suppliers\n          })\n      }\n    },\n    addSupplier() {\n      if (this.SName) {\n        let endpoint = `/api/supplier/`;\n        apiService(endpoint, \"POST\", { \n          txtSupplierName: this.SName,\n          txtStreet1: this.Street1,\n          txtStreet2: this.Street2,\n          txtStreet3: this.Street3,\n          txtZip: this.Zip,\n          txtCity: this.City,\n          txtProvince: this.Province,\n          txtLand: this.Land,\n          txtContactPerson: this.CPerson,\n          txtCpPhone: this.Phone,\n          txtCpMail: this.Mail,\n          txtCpSkype: this.Skype\n          })\n          .then(data => {\n            if (data){\n              this.suppliers.unshift(data)\n            } else {\n              this.error = true;\n            }\n          })\n        this.SName = null;\n        if (this.error) {\n          this.error = false;\n        }\n        } else {\n          this.error = true;\n        }\n        this.emptyInput()\n    },\n    openModal(params) {\n      this.$bvModal\n        .msgBoxConfirm(\"Are you sure you want to delete the selected Supplier?\", {\n          title: \"Please Confirm\",\n          size: \"sm\",\n          buttonSize: \"sm\",\n          okVariant: \"danger\",\n          okTitle: \"YES\",\n          cancelTitle: \"NO\",\n          footerClass: \"p-2\",\n          hideHeaderClose: false,\n          centered: true\n        })\n        .then(value => {\n          if (value) {\n            this.deleteSupplier();\n          }\n        })\n    },\n    async deleteSupplier() {\n      var selectedRows = this.$refs['supplier-table'].selectedRows;\n      for (var i = 0; i < selectedRows.length; i++) {\n        let endpoint = `/api/supplier/${selectedRows[i].id}/`;\n        try {\n          this.$delete(this.suppliers, selectedRows[i].originalIndex)\n          await apiService(endpoint, \"DELETE\")\n        }\n        catch (err) {\n          console.log(err)\n        }\n      }\n    },\n    emptyInput() {\n      this.SName = '',\n      this.Street1 = '',\n      this.Street2 = '',\n      this.Street3 = '',\n      this.Zip = '',\n      this.City = '',\n      this.Province = '',\n      this.Land = '',\n      this.CPerson = '',\n      this.Phone = '',\n      this.Mail = '',\n      this.Skype = ''\n    }\n\n  },\n  created() {\n    this.getSuppliers()\n  }\n};\n</script>\n<style>\n  .echarts {\n    width: 100%;\n    height: 100%;\n  }\n  i:focus,\n  input:focus,\n  select:focus,\n  textarea:focus,\n  button:focus {\n      outline: none;\n  }\n</style>\n"]}]}