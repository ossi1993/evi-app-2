         if (data) {\n              console.log(data)\n              this.models.unshift(data)\n            } else {\n              this.error = true;\n            }\n          })\n        this.MName = null;\n        if (this.error) {\n          this.error = false;\n        }\n        } else {\n          this.error = true;\n        }\n      this.emptyInput();\n    },\n    async updateModel() {\n      var selectedRows = this.$refs['model-table'].selectedRows;\n      for (var i = 0; i < selectedRows.length; i++) {\n        let endpoint = `/api/model/${selectedRows[i].id}/`;\n        try {\n          this.models[selectedRows[i].originalIndex].txtModelName = this.NewMName\n          await apiService(endpoint, \"PUT\", { \n            txtModelName: this.NewMName,\n            idDevice: this.NewDevice\n            })\n          this.hideEditModel();\n        }\n        catch (err) {\n          console.log(err)\n        }\n      }\n    },\n    openModal(params) {\n      this.$bvModal\n        .msgBoxConfirm(\"Are you sure you want to delete the selected Model?\", {\n          title: \"Please Confirm\",\n          size: \"sm\",\n          buttonSize: \"sm\",\n          okVariant: \"danger\",\n          okTitle: \"YES\",\n          cancelTitle: \"NO\",\n          footerClass: \"p-2\",\n          hideHeaderClose: false,\n          centered: true\n        })\n        .then(value => {\n          if (value) {\n            this.deleteModel();\n          }\n        })\n    },\n    async deleteModel() {\n      var selectedRows = this.$refs['model-table'].selectedRows;\n      for (var i = 0; i < selectedRows.length; i++) {\n        let endpoint = `/api/model/${selectedRows[i].id}/`;\n        try {\n          this.$delete(this.models, selectedRows[i].originalIndex)\n          await apiService(endpoint, \"DELETE\")\n        }\n        catch (err) {\n          console.log(err)\n        }\n      }\n    },\n    editModel() {\n      var selectedRows = this.$refs['model-table'].selectedRows;\n      if (selectedRows.length == 1) {\n        this.NewMName = selectedRows[0].txtModelName\n        this.NewDevice = selectedRows[0].idDevice.id\n        this.$root.$emit('bv::toggle::collapse', 'collapse-u');\n      } else {\n        this.$bvModal.show(\"modal-2\");\n      }\n    },\n    hideEditModel() {\n      this.NewMName = '';\n      this.NewDevice = null,\n      this.$root.$emit('bv::toggle::collapse', 'collapse-u')\n    },\n    emptyInput() {\n      this.MName = '',\n      this.Device = null,\n      this.$root.$emit('bv::toggle::collapse', 'collapse-d')\n    }\n\n  },\n  created() {\n    this.getModels();\n    this.getDevices();\n  }\n};\n</script>\n<style>\n  .echarts {\n    width: 100%;\n    height: 100%;\n  }\n  i:focus,\n  input:focus,\n  select:focus,\n  textarea:focus,\n  button:focus {\n      outline: none;\n  }\n</style>\n"]}]}