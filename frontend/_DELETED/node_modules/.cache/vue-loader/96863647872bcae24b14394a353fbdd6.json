;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8TA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"progressbar.vue","sourceRoot":"src/views/app/ui-kits","sourcesContent":["<template>\n  <div class=\"main-content\">\n    <breadcumb :page=\"'Progress Bar'\" :folder=\"'UI Kits'\" />\n\n    <b-row class=\" mb-30\">\n      <!-- left-side -->\n      <b-col md=\"6\">\n        <b-row>\n          <!-- start::basic-progressbar -->\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Basic Progress Bar\">\n              <h5>Default width</h5>\n              <b-progress :value=\"value\" class=\"mb-3\"></b-progress>\n\n              <h5>Custom widths</h5>\n              <b-progress :value=\"value\" class=\"w-75 mb-2\"></b-progress>\n              <b-progress :value=\"value\" class=\"w-50 mb-2\"></b-progress>\n              <b-progress :value=\"value\" class=\"w-25\"></b-progress>\n            </b-card>\n          </b-col>\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Basic Progress Bar With diff. Background\">\n              <b-progress\n                variant=\"success\"\n                :value=\"value\"\n                class=\"mb-3\"\n              ></b-progress>\n              <b-progress\n                variant=\"primary\"\n                :value=\"value\"\n                class=\"w-75 mb-2\"\n              ></b-progress>\n              <b-progress\n                variant=\"danger\"\n                :value=\"value\"\n                class=\"w-50 mb-2\"\n              ></b-progress>\n              <b-progress\n                variant=\"warning\"\n                :value=\"value\"\n                class=\"w-25\"\n              ></b-progress>\n            </b-card>\n          </b-col>\n\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Custom progress label\">\n              <h6>Custom label via default slot</h6>\n              <b-progress :max=\"max\" height=\"2rem\">\n                <b-progress-bar variant=\"danger\" :value=\"value\">\n                  Progress: <strong>{{ value.toFixed(2) }} / {{ max }}</strong>\n                </b-progress-bar>\n              </b-progress>\n\n              <h6 class=\"mt-3\">Custom label via property</h6>\n              <b-progress :max=\"max\">\n                <b-progress-bar\n                  variant=\"success\"\n                  :value=\"value\"\n                  :label=\"`${((value / max) * 100).toFixed(2)}%`\"\n                ></b-progress-bar>\n              </b-progress>\n\n              <h6 class=\"mt-3\">Custom label via property (HTML support)</h6>\n              <b-progress :max=\"max\">\n                <b-progress-bar\n                  :value=\"value\"\n                  :label-html=\"`<del>${value}</del>`\"\n                ></b-progress-bar>\n              </b-progress>\n            </b-card>\n          </b-col>\n        </b-row>\n\n        <b-row>\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Progres Bar Animated\">\n              <b-progress\n                :value=\"changeValue\"\n                :max=\"max\"\n                show-progress\n                animated\n              ></b-progress>\n              <b-progress class=\"mt-2\" :max=\"changeMax\" show-value>\n                <b-progress-bar\n                  :value=\"changeValue * (6 / 10)\"\n                  variant=\"success\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  :value=\"changeValue * (2.5 / 10)\"\n                  variant=\"warning\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  :value=\"changeValue * (1.5 / 10)\"\n                  variant=\"danger\"\n                ></b-progress-bar>\n              </b-progress>\n\n              <b-button class=\"mt-3\" @click=\"rendomValue\">Click me</b-button>\n            </b-card>\n          </b-col>\n        </b-row>\n\n        <b-row>\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Animated backgrounds\">\n             \n\n              <b-progress\n                :value=\"animatedValue\"\n                variant=\"success\"\n                striped\n                :animated=\"animate\"\n              ></b-progress>\n              <b-progress\n                :value=\"animatedValue\"\n                variant=\"info\"\n                striped\n                :animated=\"animate\"\n                class=\"mt-2\"\n              ></b-progress>\n              <b-progress\n                :value=\"animatedValue\"\n                variant=\"warning\"\n                striped\n                :animated=\"animate\"\n                class=\"mt-2\"\n              ></b-progress>\n              <b-progress\n                :value=\"animatedValue\"\n                variant=\"danger\"\n                :animated=\"animate\"\n                class=\"mt-3\"\n              ></b-progress>\n\n              <b-button\n                variant=\"secondary\"\n                @click=\"animate = !animate\"\n                class=\"mt-3\"\n              >\n                {{ animate ? \"Stop\" : \"Start\" }} Animation\n              </b-button>\n            </b-card>\n          </b-col>\n        </b-row>\n      </b-col>\n\n      <!-- right-side -->\n      <b-col md=\"6\">\n        <b-row>\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Basic Progress Bar With Labels\">\n              \n\n              <h6>No label</h6>\n              <b-progress :value=\"value\" :max=\"max\" class=\"mb-3\"></b-progress>\n\n              <h6>Value label</h6>\n              <b-progress\n                :value=\"value\"\n                :max=\"max\"\n                show-value\n                class=\"mb-3\"\n              ></b-progress>\n\n              <h6>Progress label</h6>\n              <b-progress\n                :value=\"value\"\n                :max=\"max\"\n                show-progress\n                class=\"mb-3\"\n              ></b-progress>\n\n              <h6>Value label with precision</h6>\n              <b-progress\n                :value=\"value\"\n                :max=\"max\"\n                :precision=\"2\"\n                show-value\n                class=\"mb-3\"\n              ></b-progress>\n\n              <h6>Progress label with precision</h6>\n              <b-progress\n                :value=\"value\"\n                :max=\"max\"\n                :precision=\"2\"\n                show-progress\n                class=\"mb-3\"\n              ></b-progress>\n            </b-card>\n          </b-col>\n\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Solid background variants\">\n              <div v-for=\"(bar, key) in bars\" :key=\"key\" class=\"row mb-1\">\n                <div class=\"col-sm-3\">{{ bar.variant }}:</div>\n                <div class=\"col-sm-9 pt-1\">\n                  <b-progress\n                    :value=\"bar.value\"\n                    :variant=\"bar.variant\"\n                    :key=\"bar.variant\"\n                  ></b-progress>\n                </div>\n              </div>\n            </b-card>\n          </b-col>\n\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Multiple bars\">\n              <b-progress :max=\"multipleMax\" class=\"mb-3\">\n                <b-progress-bar\n                  variant=\"primary\"\n                  :value=\"multipleValues[0]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"danger\"\n                  :value=\"multipleValues[1]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"warning\"\n                  :value=\"multipleValues[2]\"\n                ></b-progress-bar>\n              </b-progress>\n\n              <b-progress show-progress :max=\"multipleMax\" class=\"mb-3\">\n                <b-progress-bar\n                  variant=\"danger\"\n                  :value=\"multipleValues[0]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"success\"\n                  :value=\"multipleValues[1]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"dark\"\n                  :value=\"multipleValues[2]\"\n                ></b-progress-bar>\n              </b-progress>\n\n              <b-progress show-value striped :max=\"multipleMax\" class=\"mb-3\">\n                <b-progress-bar\n                  variant=\"info\"\n                  :value=\"multipleValues[0]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"success\"\n                  :value=\"multipleValues[1]\"\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"danger\"\n                  :value=\"multipleValues[2]\"\n                ></b-progress-bar>\n              </b-progress>\n\n              <b-progress :max=\"multipleMax\">\n                <b-progress-bar\n                  variant=\"primary\"\n                  :value=\"multipleValues[0]\"\n                  show-progress\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"warning\"\n                  :value=\"multipleValues[1]\"\n                  animated\n                  show-progress\n                ></b-progress-bar>\n                <b-progress-bar\n                  variant=\"info\"\n                  :value=\"multipleValues[2]\"\n                  striped\n                  show-progress\n                ></b-progress-bar>\n              </b-progress>\n            </b-card>\n          </b-col>\n          <b-col md=\"12\" class=\" mb-30\">\n            <b-card title=\"Progressbar Height\">\n            \n              <div>\n                <h5>Default height</h5>\n                <b-progress\n                  :value=\"value\"\n                  show-progress\n                  class=\"mb-3\"\n                ></b-progress>\n\n                <h5>Custom heights</h5>\n                <b-progress\n                  variant=\"success\"\n                  striped\n                  height=\"2rem\"\n                  :value=\"value\"\n                  show-progress\n                  class=\"mb-2\"\n                ></b-progress>\n                <b-progress\n                  variant=\"danger\"\n                  animated\n                  height=\"20px\"\n                  :value=\"value\"\n                  show-progress\n                  class=\"mb-2\"\n                ></b-progress>\n                <b-progress\n                  variant=\"warning\"\n                  height=\"2px\"\n                  :value=\"value\"\n                ></b-progress>\n              </div>\n            </b-card>\n          </b-col>\n        </b-row>\n      </b-col>\n    </b-row>\n  </div>\n</template>\n<script>\nexport default {\n  metaInfo: {\n    // if no subcomponents specify a metaInfo.title, this title will be used\n    title: \"ProgressBar\"\n  },\n  data() {\n    return {\n      animatedValue: 75,\n      //   animated-background\n      animate: true,\n      //   basic-progressbar value\n      value: 75,\n\n      // custom-progress-label\n      value: 33.333333333,\n      max: 50,\n\n      // solid-background-variants\n      key: 1,\n      bars: [\n        { variant: \"success\", value: 75 },\n        { variant: \"info\", value: 75 },\n        { variant: \"warning\", value: 75 },\n        { variant: \"danger\", value: 75 },\n        { variant: \"primary\", value: 75 },\n        { variant: \"secondary\", value: 75 },\n        { variant: \"dark\", value: 75 }\n      ],\n      timer: null,\n\n      // progressbar animated\n      changeValue: 45,\n      changeMax: 100,\n\n      // multiple-bar\n      multipleValues: [15, 30, 20],\n      multipleMax: 100\n    };\n  },\n  methods: {\n    rendomValue() {\n      this.changeValue = Math.random() * this.max;\n    }\n  },\n  mounted() {\n    this.timer = setInterval(() => {\n      this.bars.forEach(bar => (bar.value = 25 + Math.random() * 75));\n    }, 2000);\n  },\n  beforeDestroy() {\n    clearInterval(this.timer);\n    this.timer = null;\n  }\n};\n</script>"]}]}