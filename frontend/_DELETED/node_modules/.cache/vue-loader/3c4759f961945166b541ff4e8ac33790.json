    })\n      }\n    },\n    addManufacturer() {\n      if (this.MName) {\n        let endpoint = `/api/manufacturer/`;\n        apiService(endpoint, \"POST\", { txtManufacturerName: this.MName })\n          .then(data => {\n            if (data){\n              this.manufacturers.unshift(data)\n            } else {\n              this.error = true;\n            }\n          })\n        this.MName = null;\n        if (this.error) {\n          this.error = false;\n        }\n        } else {\n          this.error = true;\n        }\n    },\n    async updateManufacturer() {\n      var selectedRows = this.$refs['manufacturer-table'].selectedRows;\n      for (var i = 0; i < selectedRows.length; i++) {\n        let endpoint = `/api/manufacturer/${selectedRows[i].id}/`;\n        try {\n          this.manufacturers[selectedRows[i].originalIndex].txtManufacturerName = this.NewMName\n          await apiService(endpoint, \"PUT\", { txtManufacturerName: this.NewMName })\n          this.hideEditManufacturer();\n        }\n        catch (err) {\n          console.log(err)\n        }\n      }\n    },\n    openModal(params) {\n      this.$bvModal\n        .msgBoxConfirm(\"Are you sure you want to delete the selected Manufacturer?\", {\n          title: \"Please Confirm\",\n          size: \"sm\",\n          buttonSize: \"sm\",\n          okVariant: \"danger\",\n          okTitle: \"YES\",\n          cancelTitle: \"NO\",\n          footerClass: \"p-2\",\n          hideHeaderClose: false,\n          centered: true\n        })\n        .then(value => {\n          if (value) {\n            this.deleteManufacturer();\n          }\n        })\n    },\n    async deleteManufacturer() {\n      var selectedRows = this.$refs['manufacturer-table'].selectedRows;\n      for (var i = 0; i < selectedRows.length; i++) {\n        let endpoint = `/api/manufacturer/${selectedRows[i].id}/`;\n        try {\n          this.$delete(this.manufacturers, selectedRows[i].originalIndex)\n          await apiService(endpoint, \"DELETE\")\n        }\n        catch (err) {\n          console.log(err)\n        }\n      }\n    },\n    editManufacturer() {\n      var selectedRows = this.$refs['manufacturer-table'].selectedRows;\n      if (selectedRows.length == 1) {\n        this.NewMName = selectedRows[0].txtManufacturerName\n        this.$root.$emit('bv::toggle::collapse', 'collapse-u');\n      } else {\n        this.$bvModal.show(\"modal-2\");\n      }\n    },\n    hideEditManufacturer() {\n      this.NewMName = '';\n      this.$root.$emit('bv::toggle::collapse', 'collapse-u')\n    }\n  },\n  created() {\n    this.getManufacturers()\n  }\n};\n</script>\n<style>\n  .echarts {\n    width: 100%;\n    height: 100%;\n  }\n  i:focus,\n  input:focus,\n  select:focus,\n  textarea:focus,\n  button:focus {\n      outline: none;\n  }\n</style>\n"]}]}